.vessel{
    width: 100%;
    height: 90%;
    background-color: rgb(245, 245, 245);
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    background-clip: content-box;
    padding: 10px;
}
.title{
    width: 100%;
    font-size: 25px;
    margin-bottom: 5px;
}
.title::before{
    content: "*";
    color: red;
}
.content{
    font-size: 18px;
}
.van-loading{
    position: absolute;
    top:50%;
    left: 50%;
    transform: translate(-50%,-50%);
}